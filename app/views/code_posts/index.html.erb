<table style="width:100%">
  <tr>
    <th>Title</th>
    <th>Category</th>
    <th>Code Language</th>
    <th>Upvotes</th>
    <th>Downvotes</th>
  </tr>
  <% @code_posts.each do |code_post| %>
  <tr>
    <td><%= link_to code_post.title, show_code_path(code_post) %></td>
    <td><%= code_post.category %></td>
    <td><%= code_post.code_language %></td>
    <td><%= code_post.repup %></td>
    <td><%= code_post.repdown %></td>
  </tr>
  <% end %>
</table>
